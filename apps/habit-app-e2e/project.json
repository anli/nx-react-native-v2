{
  "root": "apps/habit-app-e2e",
  "sourceRoot": "apps/habit-app-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e-build-ios": {
      "executor": "@nrwl/detox:build",
      "options": {
        "detoxConfiguration": "ios.sim.debug"
      },
      "configurations": {
        "production": {
          "detoxConfiguration": "ios.sim.release"
        }
      }
    },
    "e2e-test-ios": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "nx test-ios habit-app-e2e"
      },
      "configurations": {
        "production": {
          "command": "ENVFILE=.env.production nx test-ios habit-app-e2e --configuration=production"
        }
      }
    },
    "test-ios": {
      "executor": "@nrwl/detox:test",
      "options": {
        "detoxConfiguration": "ios.sim.debug",
        "buildTarget": "habit-app-e2e:e2e-build-ios"
      },
      "configurations": {
        "production": {
          "loglevel": "verbose",
          "artifactsLocation": "./../../artifacts/",
          "recordVideos": "failing",
          "detoxConfiguration": "ios.sim.release",
          "buildTarget": "habit-app-e2e:e2e-build-ios:prod"
        }
      }
    },
    "e2e-build-android": {
      "executor": "@nrwl/detox:build",
      "options": {
        "detoxConfiguration": "android.emu.debug"
      },
      "configurations": {
        "production": {
          "detoxConfiguration": "android.emu.release"
        }
      }
    },
    "e2e-test-android": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "nx test-android habit-app-e2e"
      },
      "configurations": {
        "production": {
          "command": "ENVFILE=.env.production nx test-android habit-app-e2e --configuration=production"
        }
      }
    },
    "test-android": {
      "executor": "@nrwl/detox:test",
      "options": {
        "detoxConfiguration": "android.emu.debug",
        "buildTarget": "habit-app-e2e:e2e-build-android"
      },
      "configurations": {
        "production": {
          "loglevel": "verbose",
          "artifactsLocation": "./../../artifacts/",
          "recordVideos": "failing",
          "detoxConfiguration": "android.emu.release",
          "buildTarget": "habit-app-e2e:e2e-build-android:prod"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/habit-app-e2e/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "type-check": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "tsc -b",
        "cwd": "apps/habit-app-e2e"
      }
    },
    "pod-install": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "pod install",
        "cwd": "apps/habit-app/ios"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "habit-app"
  ]
}